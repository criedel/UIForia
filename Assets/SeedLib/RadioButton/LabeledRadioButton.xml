    <Style src="SeedLib/SeedLibTheme.style"/>
    <Style src="SeedLib/RadioButton/RadioButton.style"/>

    <Contents style="labeled-radio-button">

        <Div style="radio-button" attr:labeled="true">
            <Div attr:id="selected" if="isChecked" style="radio-button-checked"/>    
        </Div>

        <Text attr:id="text"/>

    </Contents>

</UITemplate>