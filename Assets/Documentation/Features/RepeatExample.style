style container {
    PreferredSize = 1pca;
}

style dict {
    LayoutBehavior = Ignored;
    TransformPositionY = 0;
    PreferredSize = 1pca 1cnt;
}

style entry {
    Padding = 8px 16px;
    [hover] {
        BackgroundColor = rgb(240, 240, 240);
    }
}

style next-section {
    MarginTop = 20px;
    PaddingTop = 20px;
    BorderTop = 1px;
    BorderColorTop = black;
}