<UITemplate>

    <Style>

        style container {
            TextFontSize = 24px;
            TextWhitespaceMode = PreserveNewLines;
            TextColor = white;

        }
        
        style scroll-view {
            PreferredSize = 1vw 1vh;
            LayoutFitVertical = Shrink;
        }
        
        style select {
            TextFontSize = 18px;
            PreferredWidth = 1vw;
        }
        
        style expression {
            TextColor = yellow;
            MarginBottom = 12px;
        }
        
        style mesh-test {
            PreferredSize = 200px 200px;
            // BackgroundColor = red;
            // Material = "Red";
            Border = 4px;
            BorderColor = black;
            //BackgroundImage = url("images/2");
            BorderRadius = 50%;
        }
    
    </Style>

    <Contents style="container">

        <Group style="mesh-test" 
               style:meshType="meshTest.meshType"
               style:meshFillAmount="meshTest.meshFillAmount"
               style:meshFillOrigin="meshTest.meshFillOrigin"
               style:meshFillDirection="meshTest.meshFillDirection"
        />
